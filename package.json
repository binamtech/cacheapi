{
  "name": "cacheapi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "start-dev": "cross-env NODE_ENV=development node app.js --logLevel=debug",
    "test": "cross-env SETTINGS_FILE=settings_test.yml nyc --reporter=lcov --reporter=html --reporter=json --reporter=text --report-dir=coverage node_modules/.bin/mocha --reporter spec"
  },
  "nyc": {
    "include": [
      "main",
      "app.js"
    ]
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-prettystream": "^0.1.3",
    "bunyan-rotating-file-stream": "^1.6.3",
    "cross-env": "^5.1.4",
    "express": "~4.16.0",
    "express-bunyan-logger": "^1.3.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "mocha-junit-reporter": "^1.17.0",
    "node-mocks-http": "^1.6.7",
    "nyc": "^11.6.0",
    "should": "~10.0.0",
    "sinon": "^1.17.5",
    "sinon-stub-promise": "^4.0.0",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2"
  }
}
